# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
LOAD_HARVARD_MODEL=true
ENABLE_DEEPFACE=true

# Authentication Configuration
# Set to 'true' to require users to sign in before viewing detailed analysis results
# Set to 'false' to allow anonymous users to view all results (default)
REQUIRE_AUTH=false

# Server Configuration
PORT=8001

# Railway Environment (automatically set by Railway)
RAILWAY_ENVIRONMENT=production
